
.addAddress{
	width:100%;
	min-height:100vh;
	background:#eee;
}
.adddressUls{
	background:#fff;
	padding:20rpx 25rpx;
}
.addAddressName{
	display:-webkit-box;
	display:-webkit-flex;
	display:flex;
	-webkit-box-pack:start;
	-webkit-justify-content:flex-start;
	        justify-content:flex-start;
	-webkit-box-align:center;
	-webkit-align-items:center;
	        align-items:center;
	height:90rpx;
	line-height:90rpx;
	border-bottom:1rpx solid #ddd;
}
.regionlabel{
	display:-webkit-box;
	display:-webkit-flex;
	display:flex;
	-webkit-box-pack:start;
	-webkit-justify-content:flex-start;
	        justify-content:flex-start;
	-webkit-box-align:center;
	-webkit-align-items:center;
	        align-items:center;
}
.addressregion{
	-webkit-box-pack:justify;
	-webkit-justify-content:space-between;
	        justify-content:space-between;
}
.pickerLable{
	width:300rpx;
	height:90rpx;
}
.labelName{
	font-size:26rpx;
	color:#000;
	width:120rpx;
	margin-right:10rpx;
}
.addressInput{
	height:60rpx;
	width:350rpx;
	line-height:60rpx;
	color:#000;
	font-size:26rpx;
}
.iconRight{
	font-size:28rpx;
}

